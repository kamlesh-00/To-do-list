<%- include('header') -%> 

    <div class="box" id="heading">    
        <h1><%= listTitle %></h1>
    </div>

    <div class="box">
        <% for(var i=0;i<morelihere.length;i++){ %>
        <div class="item">
            <input type="checkbox">
            <p><%= morelihere[i] %></p>
        </div>
        <% } %>   
            
        <form action="/" method="POST" class="item">
            <input type="text" name="addtask" placeholder="Add new item here" autocomplete="off" autofocus>
            <button type="submit" name="list" value="<%= listTitle %>">+</button>
        </form>
    </div>

<%- include('footer') -%> 